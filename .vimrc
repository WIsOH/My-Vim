execute pathogen#infect()
set cc=80
set autoindent
set linebreak
set showbreak=+++ 	
set textwidth=100
set showmatch
set t_Co=256
set number
set shiftwidth=4
set smartindent
set smarttab
set softtabstop=4
set nocompatible
set background=dark
syntax on
filetype plugin indent on
inoremap jk <esc>
colorscheme solarized
